<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="564.0" prefWidth="736.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OrderdetailFormcontroller">

   <JFXTreeTableView fx:id="OrderTable" layoutX="36.0" layoutY="76.0" prefHeight="400.0" prefWidth="664.0">
