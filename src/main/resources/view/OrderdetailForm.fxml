         <ImageView fx:id="png" accessibleRole="BUTTON" fitHeight="52.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../img/bck.png" />
            </image>
         </ImageView>
      </graphic>
   </JFXButton>
   <JFXButton fx:id="reportsButton" layoutX="339.0" layoutY="496.0" onAction="#reportButtonOnAction" prefHeight="36.0" prefWidth="84.0" text="Report" />
