         <TreeTableColumn fx:id="OrderID" prefWidth="163.0" text="Order ID" />
         <TreeTableColumn fx:id="itemcode_col" prefWidth="157.0" text="Item Code" />

         <TreeTableColumn fx:id="qty_col" prefWidth="152.0" text="Qty" />
